fun main()
{

    println("enter array size:")
    var size=readLine()?.toInt()!!
    println("enter elements")
    var vamshi=Array<Int>(size){ readLine()?.toInt()!! }
    var max=vamshi[0]
    for(i in vamshi) {

        println(i)
        if(i>max)
        {
            max=i
        }
    }
    println("max of the given numbers is$max")
}
